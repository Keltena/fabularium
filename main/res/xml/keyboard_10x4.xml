<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:horizontalGap="0.45%p"
    app:keyHeight="50dp"
    app:keyWidth="9.4%p"
    app:verticalGap="0.45%p">
    <Row>
        <!-- 10 keys x 9.5% + 11 gaps x 0.45% = 99.95% -->
        <Key
            app:codes="113"
            app:keyEdgeFlags="left"
            app:keyLabel="q" />
        <Key
            app:codes="119"
            app:keyLabel="w" />
        <Key
            app:codes="101"
            app:keyLabel="e" />
        <Key
            app:codes="114"
            app:keyLabel="r" />
        <Key
            app:codes="116"
            app:keyLabel="t" />
        <Key
            app:codes="121"
            app:keyLabel="y" />
        <Key
            app:codes="117"
            app:keyLabel="u" />
        <Key
            app:codes="105"
            app:keyLabel="i" />
        <Key
            app:codes="111"
            app:keyLabel="o" />
        <Key
            app:codes="112"
            app:keyEdgeFlags="right"
            app:keyLabel="p" />
    </Row>
    <Row>
        <!-- 9 keys x 9.5% + 8 gaps x 0.45% + 2 large gaps x 5.4% = 99.9% -->
        <Key
            app:codes="97"
            app:horizontalGap="5.4%p"
            app:keyEdgeFlags="left"
            app:keyLabel="a" />
        <Key
            app:codes="115"
            app:keyLabel="s" />
        <Key
            app:codes="100"
            app:keyLabel="d" />
        <Key
            app:codes="102"
            app:keyLabel="f" />
        <Key
            app:codes="103"
            app:keyLabel="g" />
        <Key
            app:codes="104"
            app:keyLabel="h" />
        <Key
            app:codes="106"
            app:keyLabel="j" />
        <Key
            app:codes="107"
            app:keyLabel="k" />
        <Key
            app:codes="108"
            app:keyEdgeFlags="right"
            app:keyLabel="l" />
    </Row>
    <Row>
        <!-- 7 normal keys x 9.5% + 2 large keys x 11% + 6 gaps x 0.45% + 2 large gaps x 4.3% = 99.8% -->
        <Key
            app:codes="8593"
            app:isRepeatable="true"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/ic_keyboard_arrow_up_white_24dp"
            app:keyWidth="11%p" />
        <Key
            app:codes="122"
            app:horizontalGap="4.3%p"
            app:keyLabel="z" />
        <Key
            app:codes="120"
            app:keyLabel="x" />
        <Key
            app:codes="99"
            app:keyLabel="c" />
        <Key
            app:codes="118"
            app:keyLabel="v" />
        <Key
            app:codes="98"
            app:keyLabel="b" />
        <Key
            app:codes="110"
            app:keyLabel="n" />
        <Key
            app:codes="109"
            app:keyLabel="m" />
        <Key
            app:codes="8"
            app:horizontalGap="4.3%p"
            app:isRepeatable="true"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_delete"
            app:keyWidth="11%p" />
    </Row>
    <Row app:rowEdgeFlags="bottom">
        <!-- 6 large keys x 11% + 1 space x 30.3% + 8 gaps x 0.45% = 99.9% -->
        <Key
            app:codes="8595"
            app:isRepeatable="true"
            app:keyEdgeFlags="left"
            app:keyIcon="@drawable/ic_keyboard_arrow_down_white_24dp"
            app:keyWidth="11%p" />
        <Key
            app:codes="0xffffffdf"
            app:keyLabel="FN"
            app:keyWidth="11%p" />
        <Key
            app:codes="44,32"
            app:keyLabel=","
            app:keyWidth="11%p" />
        <Key
            app:codes="32"
            app:isRepeatable="true"
            app:keyIcon="@drawable/sym_keyboard_space"
            app:keyWidth="30.4%p" />
        <Key
            app:codes="8592"
            app:isRepeatable="true"
            app:keyIcon="@drawable/ic_keyboard_arrow_left_white_24dp"
            app:keyWidth="11%p" />
        <Key
            app:codes="8594"
            app:isRepeatable="true"
            app:keyIcon="@drawable/ic_keyboard_arrow_right_white_24dp"
            app:keyWidth="11%p" />
        <Key
            app:codes="10"
            app:keyEdgeFlags="right"
            app:keyIcon="@drawable/sym_keyboard_return"
            app:keyWidth="11%p" />
    </Row>
</Keyboard>